mean1=38
wrong_number=36
correct_number=56
total_number=40

# sum of 40 numbers
sum1 = mean1 * total_number
print("The sum of 40 numbers:", sum1)

# correct sum of these numbers
num2 = sum1 - wrong_number + correct_number
print("Sum after correction:", num2)

# the correct mean
mean2 = num2 / total_number
print("The correct mean:", mean2)
